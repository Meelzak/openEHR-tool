        <term_definitions code="at0000">
          <items id="description">A physical or postal address for use within a health record.</items>
          <items id="text">Address</items>
        </term_definitions>
        <term_definitions code="at0001">
          <items id="comment">For example: physical or postal.</items>
          <items id="description">The type of address.</items>
          <items id="text">Type</items>
        </term_definitions>
        <term_definitions code="at0002">
          <items id="description">A physical location that can be visited.</items>
          <items id="text">Physical</items>
        </term_definitions>
        <term_definitions code="at0003">
          <items id="description">Address used as a destination for mailing letters or parcels.</items>
          <items id="text">Postal</items>
        </term_definitions>
        <term_definitions code="at0004">
          <items id="description">Address of a physical location, also used as a destination for mail.</items>
          <items id="text">Both</items>
        </term_definitions>
        <term_definitions code="at0005">
          <items id="description">The primary purpose or use for the address.</items>
          <items id="text">Use</items>
        </term_definitions>
        <term_definitions code="at0006">
          <items id="description">Address of the physical location of a business or office.</items>
          <items id="text">Business</items>
        </term_definitions>
        <term_definitions code="at0007">
          <items id="description">Address of where a person is living on a regular basis.</items>
          <items id="text">Residential</items>
        </term_definitions>
        <term_definitions code="at0008">
          <items id="description">Address of where a person is living on a temporary basis.</items>
          <items id="text">Temporary accommodation</items>
        </term_definitions>
        <term_definitions code="at0009">
          <items id="comment">This address line represents a low level of geographical/physical description of a location that, used in conjunction with the other high-level address components i.e. ‘Suburb/Town/Locality’, ‘Postcode’, and ‘State/Territory/Province’, forms a complete geographical/physical address. This data element can be used to represent a landmark, such as "The second house north of the general store" or "At the corner of Smith &amp; Brown Streets.
The content of this data element may be derived from a concatenation of one or more components from CLUSTER.structured_address.
Multiple occurrences allow for as many 'Address' lines, as required. For example: 4 address lines represented as
Apartment 7A,
52 Davis Street,
Carlton North,
Victoria, AUSTRALIA 3042.</items>
          <items id="description">The unstructured address of the person or organisation.</items>
          <items id="text">Address</items>
        </term_definitions>
        <term_definitions code="at0014">
          <items id="description">Additional structured low level geographical/physical details about a location that can be concatenated to form one or more 'Unstructured address' lines.</items>
          <items id="text">Structured address details</items>
        </term_definitions>
        <term_definitions code="at0015">
          <items id="comment">Coding with an external terminology is preferred, where possible. For example: Fitzroy, Calgary, Bergen.</items>
          <items id="description">The name of the suburb, town, city, village, community or lowest level locality of the address.</items>
          <items id="text">Town</items>
        </term_definitions>
        <term_definitions code="at0016">
          <items id="comment">Coding with an external terminology is preferred, where possible.</items>
          <items id="description">The name of an internal political or geographic district or area within a state, territory or province containing the address.</items>
          <items id="text">District/County</items>
        </term_definitions>
        <term_definitions code="at0017">
          <items id="comment">Coding with an external terminology is preferred, where possible. For example: Victoria; Alberta.</items>
          <items id="description">The name of an internal political or geographic division of a country containing the address.</items>
          <items id="text">State/Territory/Province</items>
